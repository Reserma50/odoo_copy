<odoo>
    <data>

        <!-- Define nuevas cuentas o modifica las existentes según tus necesidades -->
        <record id="account_account_expenses" model="account.account">
            <field name="name">Gastos</field>
            <field name="code">EXP</field>
            <field name="user_type_id" ref="account.data_account_type_expenses"/>
            <!-- Otros campos según tus necesidades -->
        </record>

        <record id="account_account_income_costs" model="account.account">
            <field name="name">Costos de ingreso</field>
            <field name="code">INC_COST</field>
            <field name="user_type_id" ref="account.data_account_type_expenses"/>
            <!-- Otros campos según tus necesidades -->
        </record>

        <!-- Define la jerarquía y las relaciones entre las cuentas -->
        <record id="account_group_expenses" model="account.group">
            <field name="name">Gastos</field>
            <field name="code">EXPGRP</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="account_group_income_costs" model="account.group">
            <field name="name">Costos de ingreso</field>
            <field name="code">INCCOSTGRP</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Asigna las cuentas a los grupos correspondientes -->
        <record id="account_expenses_group_relation" model="account.group.account.rel">
            <field name="group_id" ref="account_group_expenses"/>
            <field name="account_id" ref="account_account_expenses"/>
        </record>

        <record id="account_income_costs_group_relation" model="account.group.account.rel">
            <field name="group_id" ref="account_group_income_costs"/>
            <field name="account_id" ref="account_account_income_costs"/>
        </record>

    </data>
</odoo>