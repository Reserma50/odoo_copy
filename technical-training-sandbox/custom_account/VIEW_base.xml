                            <label class="fw-bold" for="remaining_hours_so"
                                string="Días restantes en la orden de venta"
                                modifiers="{&quot;invisible&quot;: [&quot;|&quot;, [&quot;encode_uom_in_days&quot;, &quot;=&quot;, false], [&quot;remaining_hours_so&quot;, &quot;&lt;&quot;, 0]]}" />
                            <label class="fw-bold text-danger" for="remaining_hours_so"
                                string="Horas restantes en la orden de venta"
                                modifiers="{&quot;invisible&quot;: [&quot;|&quot;, [&quot;encode_uom_in_days&quot;, &quot;=&quot;, true], [&quot;remaining_hours_so&quot;, &quot;&gt;=&quot;, 0]]}" />
                            <label class="fw-bold text-danger" for="remaining_hours_so"
                                string="Días restantes en la orden de venta"
                                modifiers="{&quot;invisible&quot;: [&quot;|&quot;, [&quot;encode_uom_in_days&quot;, &quot;=&quot;, false], [&quot;remaining_hours_so&quot;, &quot;&gt;=&quot;, 0]]}" />
