<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sale_order_propuesta_tree" model="ir.ui.view">
            <field name="name">sale.order.view.inherit.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='myannex']" position="replace">
                    <page string="Propuesta Ejecutiva" name="mypropuesta">
                        
                        <group>
                            <field name="propuesta_ejecutiva" string="Propuesta"/>
                            <field name="consecutivo" string="Consecutivo"/>
                        </group>
                    </page>                            
                </xpath>
            </field>
        </record>
    </data>
</odoo>