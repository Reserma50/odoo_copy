    </record>

    <record id="action_report_proposal_v2" model="ir.actions.report">
        <field name="name">Propuesta Ejecutiva cambios</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sale_fields.report_proposal_data_executive</field>
        <field name="report_file">sale_fields.report_proposal_data_executive</field>
        <field name="print_report_name">'Proposal_executive'</field>
        <!-- <field name="paperformat_id" ref="event.paperformat_euro_lowmargin"/> -->
        <field name="binding_model_id" ref="model_sale_order"/>
        <field name="binding_type">report</field>
    </record>
</odoo>