<odoo>
    <data>
        <record id="view_partner_form" model="ir.ui.view">
            <field name="name">inherited.model.form.inherit.test.partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <!-- add manytomany field to lote form -->
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="locations_ids" widget="many2many_tags" options="{'no_create': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>