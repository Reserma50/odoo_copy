<odoo>
    <data>
        <record id="view_stock_quant_tree_editable_change" model="ir.ui.view">
            <field name="name">inherited.model.tree.inherit.test</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
            <field name="arch" type="xml">
                    <xpath expr="//tree/field[@name='product_id']" position="after">
                        <!-- <field name="product_id" related="product_id.name"/> -->
                        <field name="marca_rl"/>
                        <field name="modelo_rl"/>
                        <!-- descomentar -->
                        <!-- <field name="garantia_rl"/> -->
                        <field name="ficha_rl"/>
                        <field name="install_rl"/>
                        <!-- <field name="product_id" string="name"/>
                        <field name="lot_id" context="{'default_product_id': product_id, 'default_company_id': company_id}" string="Garantia" related="lot_id.garantia"/> -->
                    </xpath>
                    <xpath expr="//tree/field[@name='location_id']" position="after">
                        <field name="region_rl"></field>
                        <field name="tipo_rl"></field>
                    </xpath>
            </field>
        </record>
    </data>
</odoo>