<odoo>
    <data>
        <record id="hr_equipment_request_view_form" model="ir.ui.view">
            <field name="name">inherited.model.form.inherit.test.maintenance</field>
            <field name="model">maintenance.request</field>
            <field name="inherit_id" ref="maintenance.hr_equipment_request_view_form"/>
            <field name="arch" type="xml">
                <!-- add manytomany field to lote form -->
                <xpath expr="//field[@name='maintenance_type']" position="after">
                    <field name="partner_id" options="{'no_create': True}"/>
                    <field name="locationmain_id" options="{'no_create': True}"/>
                </xpath>
                <xpath expr="//field[@name='user_id']" position="after">
                    <field name="tecnicos_ids" widget="many2many_tags" options="{'no_create': True}"/>
                </xpath>
                <!-- Add pages -->
                <xpath expr="/form[1]/sheet[1]/group[1]/group[2]/field[@name='description']" position="after">
                    <notebook>
                        <page string="Order Lines" name="order_lines">

                        </page>
                        <page string="Other Info" name="other_information">

                        </page>
                        
                    </notebook>
                </xpath>
            </field>
        </record>
    </data>
</odoo>