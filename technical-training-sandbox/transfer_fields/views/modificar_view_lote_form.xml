<odoo>
    <data>
        <record id="view_production_lot_form" model="ir.ui.view">
            <field name="name">inherited.model.form.inherit.test.lot</field>
            <field name="model">stock.lot</field>
            <field name="inherit_id" ref="stock.view_production_lot_form"/>
            <field name="arch" type="xml">

                <!-- Add pages -->
                <xpath expr="/form[1]/sheet[1]/notebook[1]/page[@name='description']" position="after">
                        <page string="Contacto" name="contact">
                            
                        </page>
                        <page string="Datos Madre" name="other_information">
                            <group name="First Info">
                                <group string="Ticket">
                                    <field name="ficha"/>
                                    <field name="state_equipment" options="{'no_create': True}"/>
                                    <field name="modalidad_encar" options="{'no_create': True}"/>
                                    <field name="install_date" options="{'no_create': True}"/>
                                    <field name="contact" options="{'no_create': True}"/>
                                    <field name="tel_contact" options="{'no_create': True}"/>

                                </group>
                                <group string="Compras">
                                    <group>
                                        <field name="garantia_fab_inicio"></field>
                                        <field name="garantia_fab_fin"></field>
                                        <field name="garantia_fab"></field>
                                    </group>
                                    <group>
                                        <field name="gar_extend"></field>
                                        <field name="vencimiento_gar_extend"></field>
                                    </group>
                                </group>
                                
                                <!-- <group string="Ventas/Contrato"> -->

                                    <field name="sale_ids">
                                        <tree string="Ventas/Contratos relacionados">
                                            <field name="name"/>
                                            <!-- NAME = S00{ID}  -->
                                            <field name="partner_id" string="Cliente"/>
                                            <field name="is_suscription" string="Es suscripciÃ³n?"/>
                                            
                                            <field name="warranty_duration" string="Garantia"/>
                                            <field name="contract_oc"/>
                                            <field name="inicio_contrato"/>
                                            <field name="frecuencia"/>
                                        </tree>
                                    </field>
                                    <!-- <field name="sale_id" options="{'no_create': True}"/> -->
                                    <!-- Descomentar -->
                                    <!-- <field name="cliente_id_rl"/> -->
                                    <!-- <field name="garantia_rl" options="{'no_create': True}"/> -->
                                    <!-- <field name="contrat_oc_rl" options="{'no_create': True}"/> -->
                                    <!-- <field name="fecha_contrato_rl" options="{'no_create': True}"/> -->
                                    <!-- <field name="frecuencia_rl" options="{'no_create': True}"/> -->
                                    <!-- <field name="contrato_old_rl" options="{'no_create': True}"/> -->

                                    <!-- Add ids from sale_order -->

                                <!-- </group> -->
                            </group>
                        </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>